e":401,"indent":1,"parameters":["we need to do it know! Or else someone"]},{"code":401,"indent":1,"parameters":["might discover the exchange. Meet me at the"]},{"code":401,"indent":1,"parameters":["Casino! I don't care, just come as soon as "]},{"code":401,"indent":1,"parameters":["possible! I’ll wait for you.”"]},{"code":222,"indent":1,"parameters":[]},{"code":121,"indent":1,"parameters":[24,24,0]},{"code":121,"indent":1,"parameters":[25,25,0]},{"code":201,"indent":1,"parameters":[0,19,1,24,0,0]},{"code":0,"indent":1,"parameters":[]},{"code":412,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":1,"walkAnime":false}],"x":4,"y":3}
